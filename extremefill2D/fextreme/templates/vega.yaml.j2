---
width: 500
height: 500

axes:
  - type: x
    scale: x
  - type: y
    scale: y

scales:
  - name: x
    nice: true
    range: width
    domain:
      data: contour_data0
      field: x
  - name: y
    nice: true
    range: height
    domain:
      data: contour_data0
      field: y

marks:
{%- for datum in data %}
  - type: line
    from:
      data: {{ datum.name }}
    properties:
      enter:
        x:
          scale: x
          field: x
        y:
          scale: y
          field: y
        stroke:
          value: steelblue
{%- endfor %}
data:
{{ data | to_yaml() }}
